{
  "workbench.startupEditor": "newUntitledFile",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 4,
  "editor.insertSpaces": false,
  "files.trimTrailingWhitespace": true,
  "editor.fontFamily": "Ligconsolata, Inconsolata, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "Ligconsolata, 'Inconsolata Bold', Menlo, Monaco, 'Courier New', monospace",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.formatOnPaste": true,
  "files.associations": {
    "*.jenkinsfile": "groovy"
  },
  "cSpell.userWords": ["Velostat", "hackathon", "println"],
  "arduino.logLevel": "info",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.stickyScroll.enabled": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "vscode-icons",
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools",
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false
  },
  "sqltools.results.reuseTabs": "never",
  "redhat.telemetry.enabled": false,
  "vsicons.dontShowNewVersionMessage": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true,
    "editor.formatOnSaveMode": "file",
    "editor.formatOnPaste": false
  },
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "pasteAndIndent.selectAfter": true,
  "python.formatting.provider": "black",
  "[terraform]": { "editor.formatOnSave": true },
  "[terraform-vars]": {
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "leetcode.workspaceFolder": "/Users/home/.leetcode",
  "leetcode.hint.commandShortcut": false,
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.defaultLanguage": "typescript",
  "leetcode.hint.commentDescription": false,
  "remote.SSH.useLocalServer": false,
  "remote.SSH.remotePlatform": {
    "raspberrypi.local": "linux"
  }
}
